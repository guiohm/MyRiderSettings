<application>
  <component name="XDebuggerSettings">
    <data-views>
      <option name="valueLookupDelay" value="150" />
    </data-views>
    <general>
      <scroll-to-center>true</scroll-to-center>
    </general>
    <debuggers>
      <debugger id="dotnet_debugger">
        <configuration>
          <option name="breakOnUserUnhandledExceptions" value="true" />
          <option name="enableBackendDebug" value="false" />
          <option name="enableWasmDebugging" value="false" />
        </configuration>
      </debugger>
      <debugger id="ObjectiveC">
        <configuration>
          <nt-symbol-settings />
        </configuration>
      </debugger>
      <debugger id="javascript">
        <configuration>
          <custom-object-presentation />
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>